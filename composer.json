{
  "name": "heimrichhannot/contao-no-ui-slider-bundle",
  "type": "contao-bundle",
  "description": "Extension for heimrichhannot/contao-filter-bundle that extends choice field with range appearance",
  "license": "LGPL-3.0-or-later",
  "require": {
    "php": "^7.1",
    "contao/core-bundle": "^4.4",
    "heimrichhannot/contao-utils-bundle": "^2.97"
  },
  "conflict": {
    "heimrichhannot/contao-encore-bundle": "<1.5"
  },
  "autoload": {
    "psr-4": {
      "HeimrichHannot\\NoUiSliderBundle\\": "src/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "contao-manager-plugin": "HeimrichHannot\\NoUiSliderBundle\\ContaoManager\\Plugin",
    "foxy": true
  }
}
